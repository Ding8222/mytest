<?xml version="1.0" encoding="UTF-8"?>
<!--
服务器模块参数：

Server_ID			本服的服务器ID
Listen_Port			服务器监听端口
Server_IP			本服的外网IP


DBList：			所有的库都需要填写
DBName              库名
DBUser              用户名
DBPass              密码
DBIP                IP
SQL                 要调用的sql语句
FieldName           字段名

-->

<NameCheckServer 
	Server_ID = "7000"
	Listen_Port = "7001"
	Server_IP = "127.0.0.1"	
	>
	
</NameCheckServer>

<DBList>
	<DBInfo DBName = "test" DBUser = "root" DBPass = "root" DBIP = "127.0.0.1" SQL = "select name from playerdate " FieldName = "name"/>
</DBList>