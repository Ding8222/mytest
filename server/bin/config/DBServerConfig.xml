<?xml version="1.0" encoding="UTF-8"?>
<!--
服务器模块参数：

Server_ID			本服的服务器ID
Listen_Port			服务器监听端口
Server_IP			本服的外网IP

Begin_MapID			新手出生地图
SQL_Log				是否开启SQL查询日志

-->

<DBServer 
	
	Server_ID = "2000"
	Listen_Port = "2001"
	Server_IP = "127.0.0.1"
	
	Begin_MapID = "1"
	SQL_Log = "true"
	
	>
	
</DBServer>
